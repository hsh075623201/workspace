public class JunitTest {
	@Autowired
	private AppContext appContext;
	
	@org.junit.Test
	public void Test(){
		System.out.println(appContext);
		EmployeeService employee = (EmployeeService) appContext.getBean("employeeService");
		employee.JunitTest();
	}
	
	

	
	
}
